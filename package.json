{
  "name": "noword-extension",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/cheerio": "^0.22.16",
    "@types/chrome": "^0.0.98",
    "@types/jest": "^24.0.0",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@uifabric/react-cards": "^0.109.32",
    "apollo-boost": "^0.4.7",
    "axios": "^0.19.2",
    "cheerio": "^1.0.0-rc.3",
    "chromium-extension-react-scripts": "0.0.4",
    "graphql": "^14.6.0",
    "jsencrypt": "^3.0.0-rc.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.15",
    "node-sass": "^4.13.1",
    "office-ui-fabric-react": "^7.98.3",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-hook-form": "^5.1.3",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:watch": "npm-watch",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "generateTypes": "apollo codegen:generate --target=typescript --tagName=gql",
    "generateTypes:watch": "apollo codegen:generate --target=typescript --tagName=gql --watch"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx,ts,tsx,scss",
      "ignore": [
        "build",
        "node_modules",
        "__generated__"
      ]
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jwt-decode": "^2.2.1",
    "apollo": "^2.25.0",
    "npm-watch": "^0.6.0",
    "ts-node": "^8.6.2"
  }
}
