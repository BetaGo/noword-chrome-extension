{
  "name": "noword-extension",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@material-ui/core": "^4.9.8",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/react": "^10.0.1",
    "@testing-library/user-event": "^10.0.1",
    "@types/cheerio": "^0.22.17",
    "@types/chrome": "^0.0.103",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.9.5",
    "@types/react": "^16.9.26",
    "@types/react-dom": "^16.9.0",
    "apollo-boost": "^0.4.7",
    "cheerio": "^1.0.0-rc.3",
    "chromium-extension-react-scripts": "0.0.5",
    "eventemitter3": "^4.0.0",
    "graphql": "^14.6.0",
    "jsencrypt": "^3.0.0-rc.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.15",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^5.1.3",
    "react-icons": "^3.9.0",
    "react-shadow": "^18.0.1",
    "react-use": "^13.27.1",
    "typescript": "~3.8.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:watch": "npm-watch",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "generateTypes": "apollo codegen:generate --target=typescript --tagName=gql",
    "generateTypes:watch": "apollo codegen:generate --target=typescript --tagName=gql --watch"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx,ts,tsx,scss",
      "ignore": [
        "build",
        "node_modules",
        "__generated__"
      ]
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jwt-decode": "^2.2.1",
    "apollo": "^2.25.0",
    "npm-watch": "^0.6.0",
    "ts-node": "^8.8.1"
  }
}
